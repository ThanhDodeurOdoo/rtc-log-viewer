<templates>
    <t t-name="SystemInfoPanel">
        <div class="system-info">
            <h3>System Information</h3>
            <table>
                <tr t-if="this.log.odooInfo() and this.log.odooInfo().server_version">
                    <td>Server Version:</td>
                    <td t-out="this.log.odooInfo().server_version" />
                </tr>
                <tr t-if="this.log.odooInfo() and this.log.odooInfo().db">
                    <td>Database:</td>
                    <td t-out="this.log.odooInfo().db" />
                </tr>
                <tr t-if="this.log.odooInfo() and this.log.odooInfo().isEnterprise !== undefined">
                    <td>Enterprise:</td>
                    <td t-out="this.log.odooInfo().isEnterprise ? 'Yes' : 'No'" />
                </tr>
            </table>
        </div>
    </t>
</templates>
